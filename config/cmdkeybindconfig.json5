{
	"enabled": true,
	// Keybinding to open the Macro editing screen
	"openMacroScreenKeybind": "key.keyboard.unknown",
	"macros": [
		{
			// Macro keybinding
			"keyName": "key.keyboard.keypad.7",
			// Key modifier
			"keyModName": "key.keyboard.unknown",
			// Command to execute
			"command": "/kubejs hand",
			// Type of Macro. Delayed, Repeating, RunNTimes, SingleUse, DisplayOnly
			"macroType": "SingleUse"
		},
		{
			// Macro keybinding
			"keyName": "key.keyboard.keypad.8",
			// Key modifier
			"keyModName": "key.keyboard.unknown",
			// Command to execute
			"command": "/reload",
			// Type of Macro. Delayed, Repeating, RunNTimes, SingleUse, DisplayOnly
			"macroType": "SingleUse"
		},
		{
			// Macro keybinding
			"keyName": "key.keyboard.keypad.4",
			// Key modifier
			"keyModName": "key.keyboard.unknown",
			// Command to execute
			"command": "/kubejs reload client_scripts",
			// Type of Macro. Delayed, Repeating, RunNTimes, SingleUse, DisplayOnly
			"macroType": "SingleUse"
		},
		{
			// Macro keybinding
			"keyName": "key.keyboard.keypad.9",
			// Key modifier
			"keyModName": "key.keyboard.unknown",
			// Command to execute
			"command": "/mbd2_editor",
			// Type of Macro. Delayed, Repeating, RunNTimes, SingleUse, DisplayOnly
			"macroType": "SingleUse"
		}
	]
}